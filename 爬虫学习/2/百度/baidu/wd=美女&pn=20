            if (!lefter && !rsnum && !contentno) {
                var date = new Date();
                date.setTime(date.getTime() + 5 * 60 * 1000);
                document.cookie = 'ISWR=' + detect.status + ';domain=.baidu.com;path=/;expires=' + date.toGMTString() + ';';
            }
        }, detect.time);
    });
</script>
